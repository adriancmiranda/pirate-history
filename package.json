{
  "name": "pirate-history",
  "author": "Adrian C. Miranda",
  "description": "Handles cross-browser history management",
  "version": "0.0.1",
  "types": "index.d.ts",
  "main": "index.js",
  "browser": "index.js",
  "module": "index",
  "homepage": "https://github.com/adriancmiranda/pirate-history",
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "report:coverage": "codecov -f test/coverage/coverage-final.json",
    "doc": "documentation build index.js -f html -o docs",
    "analyze:history": "source-map-explorer dist/pirate-history.min{.js,.map}",
    "analyze:memory": "source-map-explorer dist/pirate-history.memory.min{.js,.map}",
    "analyze:hash": "source-map-explorer dist/pirate-history.hash.min{.js,.map}",
    "analyze:state": "source-map-explorer dist/pirate-history.state.min{.js,.map}",
    "analyze:bundle": "run-s \"gzip -- --env.analyze=server\"",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "dev": "webpack-dev-server --config bin --env.run=dev --env.dev",
    "build:examples": "cd examples && node build-all",
    "build": "run-s gzip \"concat -- --env.dirty\"",
    "gzip": "npm run minify -- --env.gzip=js",
    "minify": "npm run concat -- --env.minify",
    "concat": "webpack --config bin --env.run=build --env.sign --env.target=var",
    "stats": "webpack --config bin --profile --json > stats.json",
    "test:unit": "run-s \"test:watch -- --single-run\"",
    "test:watch": "karma start test/karma.conf.js",
    "test:travis": "npm test",
    "test:appveyor": "npm test",
    "test:circleci": "npm test",
    "test": "run-s test:unit",
    "precommit": "run-s test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version"
  },
  "optionalDependencies": {
    "compression-webpack-plugin": "0.4.0",
    "webpack-bundle-analyzer": "2.8.2"
  },
  "devDependencies": {
    "babel-core": "~6.25.0",
    "babel-eslint": "~7.2.3",
    "babel-loader": "~7.1.1",
    "babel-plugin-istanbul": "~4.1.4",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "~1.6.0",
    "babel-preset-stage-1": "~6.24.1",
    "babel-register": "~6.24.1",
    "chai": "~4.0.2",
    "child-process-promise": "~2.2.1",
    "clean-webpack-plugin": "~0.1.16",
    "codecov": "~2.2.0",
    "compression-webpack-plugin": "~0.4.0",
    "conventional-commit-types": "~2.2.0",
    "documentation": "~4.0.0-rc.1",
    "eslint": "~4.1.1",
    "eslint-config-airbnb-base": "~11.2.0",
    "eslint-friendly-formatter": "~3.0.0",
    "eslint-import-resolver-webpack": "~0.8.3",
    "eslint-loader": "~1.9.0",
    "eslint-plugin-import": "~2.7.0",
    "friendly-errors-webpack-plugin": "~1.6.1",
    "git-revision-webpack-plugin": "~2.5.1",
    "html-webpack-plugin": "~2.29.0",
    "husky": "~0.14.3",
    "karma": "~1.7.0",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-firefox-launcher": "~1.0.1",
    "karma-fixture": "~0.2.6",
    "karma-html2js-preprocessor": "~1.1.0",
    "karma-json-fixtures-preprocessor": "~0.0.6",
    "karma-mocha": "~1.3.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-phantomjs-shim": "~1.4.0",
    "karma-safari-launcher": "~1.0.0",
    "karma-sinon-chai": "~1.3.1",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-spec-reporter": "~0.0.31",
    "karma-webpack": "~2.0.3",
    "mocha": "~3.4.2",
    "moment": "~2.18.1",
    "npm-check-updates": "~2.12.1",
    "npm-run-all": "~4.0.2",
    "pirate-flag": "~0.1.0",
    "pug": "~2.0.0-rc.2",
    "pug-loader": "~2.3.0",
    "sinon": "~2.3.6",
    "sinon-chai": "~2.10.0",
    "source-map-explorer": "~1.4.0",
    "standard-version": "~4.2.0",
    "strip-loader": "~0.1.2",
    "validate-commit-msg": "~2.12.2",
    "webpack": "~3.1.0",
    "webpack-cfg": "~1.2.7",
    "webpack-dev-server": "~2.5.1"
  },
  "files": [
    "index.js",
    "source",
    "dist"
  ],
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "greenkeeper": {
    "ignore": [
      "material"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/pirate-history.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/pirate-history/issues"
  },
  "engines": {
    "node": ">=6.10.1"
  },
  "keywords": [
    "pirate-history"
  ]
}
