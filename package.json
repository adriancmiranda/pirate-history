{
  "name": "pirate-history",
  "author": "Adrian C. Miranda",
  "description": "Handles cross-browser history management",
  "version": "0.0.1",
  "types": "index.d.ts",
  "main": "index.js",
  "browser": "index.js",
  "module": "index",
  "homepage": "https://github.com/adriancmiranda/pirate-history",
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "dev": "webpack-dev-server --config bin --env.run=dev --env.dev",
    "build": "webpack --config bin --env.run=build",
    "build:examples": "cd examples && node build-all",
    "stats": "webpack --config bin --profile --json > stats.json",
    "test": "mocha -R spec",
    "test:travis": "npm test",
    "test:appveyor": "npm test",
    "precommit": "run-s test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version"
  },
  "optionalDependencies": {
    "compression-webpack-plugin": "0.4.0",
    "webpack-bundle-analyzer": "2.4.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "child-process-promise": "^2.2.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^0.4.0",
    "conventional-commit-types": "^2.1.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "git-revision-webpack-plugin": "^2.4.1",
    "husky": "^0.13.3",
    "mocha": "^3.2.0",
    "moment": "^2.18.1",
    "npm-check-updates": "^2.11.0",
    "npm-run-all": "^4.0.2",
    "pirate-flag": "^0.0.1",
    "standard-version": "^4.0.0",
    "validate-commit-msg": "^2.12.1",
    "webpack": "^2.4.1",
    "webpack-cfg": "^1.2.4",
    "webpack-dev-server": "2.4.4",
    "webpack-node-externals": "1.5.4"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "greenkeeper": {
    "ignore": [
      "material"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/pirate-history.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/pirate-history/issues"
  },
  "engines": {
    "node": ">=6.10.1"
  },
  "keywords": [
    "pirate-history"
  ]
}
