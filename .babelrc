{
  "comments": false,
  "plugins": ["transform-runtime"],
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "not ie <= 8"
        ]
      }
    }],
    "stage-1"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-1"],
      "plugins": ["istanbul"]
    }
  },
  "ignore": [
    ".github/",
    "dist/",
    "examples/",
    "material/"
  ]
}
