doctype html
html(lang="pt-br")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    if htmlWebpackPlugin.options.mobile
      meta(name="viewport", content="width=device-width, initial-scale=1")
    title #{htmlWebpackPlugin.options.title || 'pirate-history'}
    link(rel="icon", type="image/x-icon", href="data:image/x-icon;base64,")
    each file in (htmlWebpackPlugin.files.styles || [])
      link(rel="stylesheet", href=`${file}`)
    each file in htmlWebpackPlugin.files.css
      link(rel="stylesheet", href=`${file}`)
    each file in (htmlWebpackPlugin.options.scripts || [])
      script(href=`${file}`)
  body.App(class=`${htmlWebpackPlugin.options.env || ''}`)
    header
      h1 #{htmlWebpackPlugin.options.title}
    section#Fixture
    each chunk in htmlWebpackPlugin.files.chunks
      script(src=`${chunk.entry}`)
